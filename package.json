{
  "name": "media",
  "version": "1.0.0",
  "description": "My great Nuxt.js project",
  "author": "yozz",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "test": "ava --verbose --fail-fast --match='gen tree'"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.0-rc4",
    "@nuxtjs/pwa": "^3.0.0-beta.12",
    "apollo-link-http": "^1.5.14",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "ffmpeg-static": "^2.4.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "graphql": "^14.1.1",
    "graphql-errors": "^2.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "iview": "^3.3.0",
    "lodash": "^4.17.11",
    "mime": "^2.4.0",
    "moment": "^2.24.0",
    "nuxt": "^2.4.5",
    "simple-thumbnail": "^1.4.5",
    "uuid-by-string": "^2.1.0",
    "vue-infinite-loading": "^2.4.3",
    "vue-lazyload": "^1.2.6",
    "vue-plyr": "^5.1.3",
    "vue-property-decorator": "^8.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.3.1",
    "babel-eslint": "^10.0.1",
    "browser-env": "^3.2.6",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.18.10",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^2.0.0",
    "supertest": "^4.0.0",
    "vue-template-compiler": "^2.6.7"
  },
  "ava": {
    "require": [
      "./test/helpers/setup.js"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
